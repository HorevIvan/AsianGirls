<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LearnNHibernate" namespace="LearnNHibernate" xmlns="urn:nhibernate-mapping-2.2">
  <class name="User" table="Users" lazy="true" >
    <id name="Number" column="Number">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="UserTypeNumber">
      <column name="UserTypeNumber" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>